{
  "rnn_units": 128,
  "lambda": 0,
  "bidir_adj_mx": true,

  "scaler": "normal",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": false,
  "add_day_in_week": false,

  "max_epoch": 200,
  "learner": "adam",
  "learning_rate": 0.001,
  "lr_decay": true,
  "lr_scheduler": "multisteplr",
  "lr_decay_ratio": 0.7,
  "steps": [5, 15, 30, 50, 70, 90, 110, 130, 150],
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": true,
  "patience": 50
}
